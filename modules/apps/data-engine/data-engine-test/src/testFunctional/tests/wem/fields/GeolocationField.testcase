	@ignore = "true"
	@priority = "4"
	test DuplicateRepeatableGeolocationOnWebContent {
		property portal.acceptance = "false";

		DataEngine.addField(
			fieldFieldLabel = "Localization",
			fieldName = "Geolocation");

		DataEngine.editFieldRepeatable(fieldFieldLabel = "Localization");

		WebContentStructures.saveCP(structureName = "My Structure Name 1");

		Navigator.gotoNavItem(navItem = "Web Content");

		LexiconEntry.gotoAdd();

		MenuItem.click(menuItem = "My Structure Name 1");

		WebContent.addCP(webContentTitle = "My Web Content Name 1");

		DataEngine.addRepeatableField(fieldName = "Localization");

		Button.clickPublish();

		WebContentNavigator.gotoEditCP(webContentTitle = "My Web Content Name 1");

		AssertElementPresent(locator1 = "FormFields#FIELD_ADD_REMOVE");
	}

