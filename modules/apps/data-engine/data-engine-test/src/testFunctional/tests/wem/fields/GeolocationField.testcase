	test VerifyDuplicatedGeolocationOnWebContent {
		property portal.acceptance = "false";

		var key_Label = "Localization";

		var key_fieldLabel = "Copy of ${key_Label}";

		DataEngine.addField(
			fieldFieldLabel = "${key_Label}",
			fieldName = "Geolocation");

		DataEngine.duplicateField(fieldFieldLabel = "${key_Label}");

		WebContentStructures.saveCP(structureName = "My Structure Name 1");

		Navigator.gotoNavItem(navItem = "Web Content");

		LexiconEntry.gotoAdd();

		MenuItem.click(menuItem = "My Structure Name 1");

		WebContent.addCP(
			webContentTitle = "My Web Content Name 1");

		Button.clickPublish();

		WebContentNavigator.gotoEditCP(webContentTitle = "My Web Content Name 1");

		AssertElementPresent(locator1 = "FormFields#LABEL");
	}

